package com.epologee.camera {	import flash.events.Event;	import flash.utils.getQualifiedClassName;	
	/**	 * @author Eric-Paul Lecluse | epologee.com (c) 2009	 */	public class CameraFetchEvent extends Event {		public static const ERROR:String = "error";		//		// public properties:		public var message:String;				public function CameraFetchEvent(inType:String, inMessage:String = "") {			super(inType, true);		}				override public function clone():Event {			var c:CameraFetchEvent = new CameraFetchEvent(type, message);			return c;		}				override public function toString():String {			// com.epologee.camera.CameraFetchEvent			return getQualifiedClassName(this);		}	}}