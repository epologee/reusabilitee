package com.epologee.time {	import com.epologee.development.logging.logger;	import flash.display.Stage;	import flash.events.TimerEvent;	import flash.utils.Timer;	import flash.utils.getQualifiedClassName;	/**	 * @author Eric-Paul Lecluse | epologee.com (c) 2009	 * 	 * If you need to test your application on different framerates, use this tester class.	 */	public class FrameRateTester {		private var mStage : Stage;		private var mStartRate : uint;		public function FrameRateTester(inStage : Stage, inStartRate : uint = 21) {			mStage = inStage;			mStartRate = inStartRate;			mStage.frameRate = inStartRate;						var t : Timer = new Timer(5000);			t.addEventListener(TimerEvent.TIMER, increaseFrameRate);			t.start();		}		private function increaseFrameRate(event : TimerEvent) : void {			if (mStage.frameRate >= 120) {				mStage.frameRate = mStartRate;			} else {				mStage.frameRate += 5;			}			logger.warn("increaseFrameRate: " + mStage.frameRate);		}		public function toString():String {			// com.epologee.time.FrameRateTester			var s:String = "";			// s = "[ " + name + " ]:";			return s+getQualifiedClassName(this);		}	}}