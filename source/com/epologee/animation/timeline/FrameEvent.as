package com.epologee.animation.timeline {	import flash.events.Event;	import flash.utils.getQualifiedClassName;		/**	 * @author Eric-Paul Lecluse | epologee.com (c) 2009	 */	public class FrameEvent extends Event {		public static const STATUS_CHANGE:String = "name";		//		// public properties:		public var status:int;		public var label:String;		public function FrameEvent(inType:String, inStatus:int, inLabel:String) {			super(inType, true);						status = inStatus;			label = inLabel;		}		override public function clone():Event {			var c:FrameEvent = new FrameEvent(type, status, label);			return c;		}		override public function toString():String {			// com.epologee.animation.timeline.FrameEvent			return getQualifiedClassName(this);		}	}}