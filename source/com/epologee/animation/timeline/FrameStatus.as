package com.epologee.animation.timeline {	/**	 * @author Eric-Paul Lecluse | epologee.com (c) 2009	 */	public class FrameStatus {		public static const FIRST_FRAME:int = 1;		public static const PRECEDING_LABEL_CHANGE:int = 2;		public static const LABEL_CHANGE:int = 4;		public static const LAST_FRAME:int = 8;		public static function getStatusDescription(inStatus:int):String {			var status:Array = [];			if (inStatus & FIRST_FRAME) status.push( "FIRST_FRAME");			if (inStatus & PRECEDING_LABEL_CHANGE) status.push( "PRECEDING_LABEL_CHANGE");			if (inStatus & LABEL_CHANGE) status.push( "LABEL_CHANGE");			if (inStatus & LAST_FRAME) status.push( "LAST_FRAME");						return "\n\t"+status.join("\n\t");					}	}}