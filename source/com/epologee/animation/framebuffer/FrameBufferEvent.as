package com.epologee.animation.framebuffer {	import flash.events.Event;	import flash.utils.getQualifiedClassName;		/**	 * @author Eric-Paul Lecluse | epologee.com © 2008	 */	public class FrameBufferEvent extends Event {		public static const BUFFERED:String = "buffered";				public function FrameBufferEvent(inType:String) {			super(inType, true);		}				override public function clone():Event {			var c:FrameBufferEvent = new FrameBufferEvent(type);			return c;		}				override public function toString():String {			// nl.noise.hero.world.rotation.FrameBufferEvent			return getQualifiedClassName(this);		}	}}