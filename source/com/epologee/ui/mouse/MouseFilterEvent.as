package com.epologee.ui.mouse {	import flash.events.Event;	import flash.utils.getQualifiedClassName;		/**	 * @author Eric-Paul Lecluse | epologee.com © 2008	 */	public class MouseFilterEvent extends Event {		public static const INACTIVITY_TIMEOUT:String = "no interactivity timeout";		public static const REACTIVATED:String = "reactivated";		public function MouseFilterEvent(inType:String) {			super(inType, true);		}				override public function clone():Event {			var c:MouseFilterEvent = new MouseFilterEvent(type);			return c;		}				override public function toString():String {			// com.epologee.ui.mouse.MouseFilterEvent			return getQualifiedClassName(this);		}	}}