package com.epologee.ui.buttons {	import flash.events.Event;	import flash.utils.getQualifiedClassName;		/**	 * @author Eric-Paul Lecluse | epologee.com ï¿½ 2009	 */	public class MultiStateEvent extends Event {		public static const CLICK:String = "multistate click";		public static const MOUSE_DOWN:String = "multistate mouse down";		public static const MOUSE_UP:String = "multistate mouse up";		public static const MOUSE_OUT : String = "multistate mouse out";		public static const MOUSE_OVER : String = "multistate mouse over";		//		// public properties:				public function MultiStateEvent(inType:String) {			super(inType, true);		}				override public function clone():Event {			var c:MultiStateEvent = new MultiStateEvent(type);			return c;		}				override public function toString():String {			// com.epologee.ui.buttons.MultiStateEvent			return getQualifiedClassName(this);		}	}}