package com.epologee.ui.combobox {	import flash.events.Event;	import flash.utils.getQualifiedClassName;		/**	 * @author Eric-Paul Lecluse | epologee.com (c) 2009	 */	public class EasyComboEvent extends Event {		public static const CHANGED : String = "combobox selection changed";		//		// public properties:		public var subtype : String;		public var data : String;		public var label : String;		public function EasyComboEvent(inType : String) {			super(inType, true);		}		override public function clone() : Event {			var c : EasyComboEvent = new EasyComboEvent(type);			return c;		}		override public function toString() : String {			// com.epologee.ui.combobox.EasyComboEvent			return getQualifiedClassName(this);		}	}}